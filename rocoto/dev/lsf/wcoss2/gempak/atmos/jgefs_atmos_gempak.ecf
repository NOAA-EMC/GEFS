##PBS -n 66
##PBS -R span[ptile=33]
##PBS -R 'affinity[core(1)]'
#PBS -q dev
#PBS -l walltime=4:30:00
##PBS -L /bin/sh
#PBS -A GEFS-DEV
#PBS -V
